<script type="text/javascript">  
  document.addEventListener("DOMContentLoaded",function(){  
    <%case @result%>
    <%when :payment_processed_already %>
      alert('onPaymentSuccess'+ '<%=flash[:notice]%>');
      console.log("onPaymentSuccess");
    <%when :order_completed%>
      alert('onPaymentSuccess'+ '<%=flash[:notice]%>');
      console.log("onPaymentSuccess");
    <%when :payment_success_but_order_incomplete%>
      alert('onPaymentFailure'+ '<%=flash[:warn]%>');
      console.log("onPaymentSuccess");
    <%when :payment_failure%>
      alert('onPaymentFailure'+ '<%=flash[:error]%>');
      console.log("onPaymentSuccess");
    <%else%>
      alert('onPaymentSuccess'+ @result);
      console.log("onPaymentSuccess");
    <%end%>  
  });
</script>

